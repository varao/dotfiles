!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!! To map Tab-hold to control (see also .xinitrc)
!!
keycode 23 = Hyper_L
!!  hyper_l is mod4 by default
remove mod4 = Hyper_L
add Control = Hyper_L
! Map space to an unused keycode (to keep it around for xcape to use).
keycode any = Tab
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

!! Remap <Caps>-hjkl to arrows
keycode 66 = Mode_switch
keysym j = j J Down 
keysym l = l L Right
keysym k = k K Up
keysym h = h H Left
keysym u = u U Prior
keysym n = n N Next
